<keymap version="1" name="Custom" parent="$default">
  <action id="CodeCompletion">
    <keyboard-shortcut first-keystroke="shift space" />
  </action>
  <action id="Console.TableResult.ColumnVisibility" />
  <action id="MoveTabRight">
    <keyboard-shortcut first-keystroke="shift alt add" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="alt f" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="shift alt 2" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
  <action id="ToggleDistractionFreeMode">
    <keyboard-shortcut first-keystroke="ctrl alt numpad2" />
  </action>
  <action id="ToggleFullScreen">
    <keyboard-shortcut first-keystroke="ctrl alt numpad3" />
  </action>
  <action id="TogglePresentationMode">
    <keyboard-shortcut first-keystroke="ctrl alt numpad1" />
  </action>
  <action id="Unsplit">
    <keyboard-shortcut first-keystroke="shift alt subtract" />
  </action>
</keymap>